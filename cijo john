        "## 9. Generate the series of dates from 1st Jan, 2023 to 10th Feb, 2023"
      ],
      "metadata": {
        "id": "UXSmdNclyJQD"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "from datetime import date as date_n  \n",
        "   \n",
        "def number_of_days(date_1, date_2):  \n",
        "    return (date_2 - date_1).days  \n",
        "       \n",
        "# Driver program  \n",
        "date_1 = date_n(2023, 1,1)  \n",
        "date_2 = date_n(2023, 2, 10)  \n",
        "print (\"Number of Days between the given Dates are: \", number_of_days(date_1, date_2), \"days\")  "
      ],
      "metadata": {
        "id": "dgyC0JhVYl4F",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "fc2b2913-3fbe-4ea2-f4f8-fbf98a3234d4"
      },
      "execution_count": 10,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Number of Days between the given Dates are:  40 days\n"
          ]
        }
